MAKE = make
RM = rm -f

all: clean libpll-AVX.a libpll-SSE3.a libpll-AVX-PTHREADS.a libpll-SSE3-PTHREADS.a

libpll-AVX.a: Makefile.AVX.gcc
	-$(RM) *.o
	$(MAKE) -f $+

libpll-SSE3.a: Makefile.SSE3.gcc
	-$(RM) *.o
	$(MAKE) -f $+

libpll-AVX-PTHREADS.a: Makefile.AVX.PTHREADS.gcc
	-$(RM) *.o
	$(MAKE) -f $+

libpll-SSE3-PTHREADS.a: Makefile.SSE3.PTHREADS.gcc
	-$(RM) *.o
	$(MAKE) -f $+

clean:
	-$(RM) *.a *.o

.PHONY: all clean
